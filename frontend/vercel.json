{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/fund",
      "destination": "/fund.html"
    },
    {
      "source": "/swap-rps",
      "destination": "/swap-rps.html"
    },
    {
      "source": "/(.*\\.json)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
